{
  "name": "vue-banana-i18n",
  "version": "2.3.0",
  "description": "A Banana-i18n wrapper to support localization in Vue.js",
  "author": {
    "name": "Santhosh Thottingal",
    "email": "santhosh.thottingal@gmail.com"
  },
  "repository": {
    "url": "git+https://github.com/santhoshtr/vue-banana-i18n.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-banana-i18n.umd.js",
  "module": "./dist/vue-banana-i18n.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue-banana-i18n.mjs",
      "require": "./dist/vue-banana-i18n.umd.js"
    }
  },
  "homepage": "github.com/santhoshtr/vue-banana-i18n",
  "keywords": [
    "vue",
    "vue.js",
    "plugin",
    "i18n",
    "internationalization"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint src test",
    "test:unit": "vitest run",
    "test:dev": "vitest",
    "test": "npm run lint && npm run test:unit"
  },
  "dependencies": {
    "banana-i18n": "^2.3.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jsdom": "^19.0.0",
    "vite": "^2.7.13",
    "vitest": "^0.2.5",
    "vue": "^3.2.28"
  }
}
